{
  "name": "patchify-bun",
  "version": "1.0.0",
  "description": "microKORG Synth Editor - Bun backend with Web Audio preview and MIDI support",
  "module": "server/index.js",
  "type": "module",
  "scripts": {
    "start": "bun run server/index.js",
    "dev": "bun --watch server/index.js",
    "build:frontend": "cd frontend && bun run build",
    "install:frontend": "cd frontend && bun install",
    "cli": "bun run cli/index.js"
  },
  "dependencies": {
    "bun": "latest",
    "easymidi": "^3.1.0",
    "jazz-midi": "^1.7.9",
    "midi": "^2.0.0",
    "pdf-parse": "^2.4.5"
  },
  "keywords": [
    "synth",
    "microkorg",
    "bun",
    "web-audio",
    "midi"
  ],
  "author": "Patchify Team",
  "license": "MIT"
}
